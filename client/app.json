{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.client",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "react-native-dotenv"
          ]
        }
      }
    },
    "jsEngine": "jsc",
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "9ff2510d-456f-486e-9b8a-88b73576b837"
      }
    },
    "owner": "andrei.sorokin.1"
  }
}
